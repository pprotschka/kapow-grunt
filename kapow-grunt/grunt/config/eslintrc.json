{
    "extends": "wordpress",
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "jQuery": true,
        "document": true,
        "module": true,
        "window": true
    },
    "rules": {
        "no-cond-assign": [
            2,
            "except-parens"
        ],
        "curly": 2,
        "eqeqeq": 2,
        "no-eq-null": 2,
        "no-unused-expressions": 2,
        "no-caller": 2,
        "no-irregular-whitespace": 2,
        "no-undef": 2,
        "no-unused-vars": 1
    }
}
